<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="120dp">    <androidx.cardview.widget.CardView        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginStart="5dp"        android:layout_marginEnd="5dp"        app:cardCornerRadius="4dp"        app:cardElevation="3dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="110dp">            <View                android:layout_width="5dp"                android:layout_height="wrap_content"                android:background="@color/blue"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/recentFTextViewCasePlaceholder"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="20dp"                android:layout_marginTop="12dp"                android:fontFamily="@font/roboto_black"                android:text="CASE #"                android:textColor="#8F9595"                android:textSize="18sp"                android:textStyle="bold"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/recentFTextViewCaseId"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="4dp"                android:layout_marginTop="12dp"                android:fontFamily="@font/roboto_black"                android:text="#5315"                android:textColor="#8F9595"                android:textSize="18sp"                android:textStyle="bold"                app:layout_constraintStart_toEndOf="@+id/recentFTextViewCasePlaceholder"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/recentFTextViewCaseDate"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="20dp"                android:layout_marginTop="2dp"                android:fontFamily="@font/roboto_medium"                android:text="Today: "                android:textColor="#D8DBD8"                android:textSize="14sp"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/recentFTextViewCaseId" />            <TextView                android:id="@+id/recentFTextViewCaseTime"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginTop="2dp"                android:fontFamily="@font/roboto_medium"                android:text="7:40pm"                android:textColor="#D8DBD8"                android:textSize="14sp"                app:layout_constraintStart_toEndOf="@+id/recentFTextViewCaseDate"                app:layout_constraintTop_toBottomOf="@+id/recentFTextViewCaseId" />            <ImageView                android:id="@+id/imageViewHistoryCaseLocation"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="4dp"                android:layout_marginTop="38dp"                app:layout_constraintStart_toEndOf="@+id/recentFTextViewCaseTime"                app:layout_constraintTop_toTopOf="parent"                app:srcCompat="@drawable/ic_baseline_location_on_24" />            <TextView                android:id="@+id/recentFTextViewCaseAddress"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="2dp"                android:layout_marginTop="38dp"                android:ellipsize="end"                android:fontFamily="@font/roboto_medium"                android:maxEms="6"                android:singleLine="true"                android:text="Barbara Parkhiohoiiiiiiiiiiiiiiiii"                android:textColor="#D8DBD8"                android:textSize="14sp"                app:layout_constraintStart_toEndOf="@+id/imageViewHistoryCaseLocation"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/recentFTextViewCondition1"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="20dp"                android:layout_marginTop="10dp"                android:background="@drawable/bg_rounded_graycorners"                android:fontFamily="@font/roboto"                android:paddingStart="15dp"                android:paddingTop="5dp"                android:paddingEnd="15dp"                android:paddingBottom="5dp"                android:text="asthma"                android:textSize="10sp"                android:textStyle="bold"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/recentFTextViewCaseTime" />            <TextView                android:id="@+id/recentFTextViewCondition2"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="5dp"                android:layout_marginTop="10dp"                android:background="@drawable/bg_rounded_graycorners"                android:paddingStart="15dp"                android:paddingTop="5dp"                android:paddingEnd="15dp"                android:paddingBottom="5dp"                android:text="repsository collapse"                android:textSize="10sp"                android:textStyle="bold"                app:layout_constraintStart_toEndOf="@+id/recentFTextViewCondition1"                app:layout_constraintTop_toBottomOf="@+id/recentFTextViewCaseTime" />            <TextView                android:id="@+id/recentFTextViewCondition3"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="5dp"                android:layout_marginTop="10dp"                android:background="@drawable/bg_rounded_graycorners"                android:paddingStart="15dp"                android:paddingTop="5dp"                android:paddingEnd="15dp"                android:paddingBottom="5dp"                android:text="peunomia"                android:textSize="10sp"                android:textStyle="bold"                app:layout_constraintStart_toEndOf="@+id/recentFTextViewCondition2"                app:layout_constraintTop_toBottomOf="@+id/recentFTextViewCaseTime" />            <!--            <ImageView-->            <!--                android:id="@+id/recentFImageViewStatus"-->            <!--                android:layout_width="wrap_content"-->            <!--                android:layout_height="wrap_content"-->            <!--                android:layout_marginTop="17dp"-->            <!--                android:layout_marginEnd="90dp"-->            <!--                android:contentDescription="@string/imageViewStatus"-->            <!--                app:layout_constraintEnd_toEndOf="parent"-->            <!--                app:layout_constraintTop_toTopOf="parent"-->            <!--                app:srcCompat="@drawable/ic_status_green_10" />-->            <!--            <TextView-->            <!--                android:id="@+id/recentFTextViewStatus"-->            <!--                android:layout_width="wrap_content"-->            <!--                android:layout_height="wrap_content"-->            <!--                android:layout_marginTop="15dp"-->            <!--                android:layout_marginEnd="30dp"-->            <!--                android:text="completed"-->            <!--                android:textAllCaps="true"-->            <!--                android:textColor="#4BCC89"-->            <!--                android:textSize="10sp"-->            <!--                app:layout_constraintEnd_toEndOf="parent"-->            <!--                app:layout_constraintTop_toTopOf="parent" />-->            <ImageButton                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginTop="40dp"                android:layout_marginEnd="20dp"                android:background="@drawable/bg_static_bluecircle"                android:padding="5dp"                android:src="@drawable/ic_baseline_arrow_forward_ios_14"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent" />        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.cardview.widget.CardView></androidx.constraintlayout.widget.ConstraintLayout>